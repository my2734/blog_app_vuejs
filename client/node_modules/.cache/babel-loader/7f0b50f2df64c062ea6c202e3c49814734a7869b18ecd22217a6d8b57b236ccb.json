{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"LoginApp\",\n  data() {\n    return {\n      user: {\n        username: \"\",\n        email: \"\",\n        password: \"\",\n        repeat_password: \"\"\n      },\n      user_login: {\n        username: \"\",\n        password: \"\"\n      }\n    };\n  },\n  methods: {\n    handleBackGoHome() {\n      this.$store.state.statusOverly = false;\n      this.$router.push({\n        path: '/'\n      });\n    },\n    submit_register() {\n      const data = {\n        username: this.user.username,\n        password: this.user.password,\n        email: this.user.email\n      };\n      axios.post('http://localhost:8080/api/auth/register', data).then(response => {\n        return response;\n      }).then(data => {\n        console.log(data);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    submit_login() {\n      const data = {\n        username: this.user_login.username,\n        password: this.user_login.password\n      };\n      console.log(data);\n    }\n  }\n};","map":{"version":3,"mappings":";AA+FA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAC;MAClB,CAAC;MACDC,UAAU,EAAC;QACTJ,QAAQ,EAAE,EAAE;QACZE,QAAQ,EAAE;MACZ;IACF;EACF,CAAC;EACDG,OAAO,EAAC;IACNC,gBAAgB,GAAE;MAChB,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,YAAW,GAAI,KAAK;MACtC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAI,CAAC,CAAC;IAClC,CAAC;IACDC,eAAe,GAAE;MACf,MAAMf,IAAG,GAAI;QACXE,QAAQ,EAAE,IAAI,CAACD,IAAI,CAACC,QAAQ;QAC5BE,QAAQ,EAAE,IAAI,CAACH,IAAI,CAACG,QAAQ;QAC5BD,KAAI,EAAI,IAAI,CAACF,IAAI,CAACE;MACpB;MAEAL,KAAK,CAACkB,IAAI,CAAC,yCAAyC,EAAChB,IAAI,EACxDiB,IAAI,CAACC,QAAO,IAAK;QAChB,OAAOA,QAAQ;MACjB,CAAC,EACAD,IAAI,CAACjB,IAAI,IAAE;QACVmB,OAAO,CAACC,GAAG,CAACpB,IAAI,CAAC;MACnB,CAAC,EACAqB,KAAK,CAAEC,KAAK,IAAG;QACdH,OAAO,CAACC,GAAG,CAACE,K