ader.vue\";\r\nimport FooterApp from \"../components/Footer.vue\";\r\nimport OverlayApp from \"@/components/Overlay.vue\";\r\nimport SideBarApp from '../components/SideBar.vue';\r\nexport default {\r\n  name: \"MainLayout\",\r\n  components: {\r\n    HeaderApp,\r\n    FooterApp,\r\n    SideBarApp,\r\n    Overlay\r\n  },\r\n  data(){\r\n    return{\r\n      status: false\r\n    }\r\n  }\r\n};\r\n</script>\r\n  \r\n  <style>\r\n</style>\r\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}