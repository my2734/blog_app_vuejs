{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"PostTags\",\n  computed: {\n    isBlack() {\n      return this.$store.state.statusBackground;\n    }\n  },\n  data() {\n    return {\n      tags: \"\",\n      isError: false,\n      list_tags: [],\n      status_update: false\n    };\n  },\n  methods: {\n    handleSubmitTags() {\n      if (!this.tags) {\n        this.isError = true;\n      }\n      axios.post(\"http://localhost:8080/api/tags\", {\n        name: this.tags\n      }).then(response => {\n        this.list_tags.push(response.data);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    handleDeleteTags(id, index) {\n      axios.delete(\"http://localhost:8080/api/tags/\" + id).then(response => {\n        response.status == 200 && this.list_tags.splice(index, 1);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    handleGetDetail(id) {\n      axios.get(\"http://localhost:8080/api/tags/\" + id).then(response => {\n        if (response.status == 200) {\n          console.log(response.data);\n          this.status_update = true;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  mounted() {\n    axios.get(\"http://localhost:8080/api/tags\").then(response => {\n      if (response.status == 200) {\n        this.list_tags = response.data;\n      }\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":";AAoLA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,QAAQ,EAAE;IACRC,OAAO,GAAG;MACR,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,gBAAgB;IAC3C;EACF,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE,EAAE;MACbC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgB,GAAG;MACjB,IAAI,CAAC,IAAI,CAACL,IAAI,EAAE;QACd,IAAI,CAACC,OAAM,GAAI,IAAI;MACrB;MAEAT,KAAI,CACDc,IAAI,CAAC,gCAAgC,EAAE;QAAEb,IAAI,EAAE,IAAI,CAACO