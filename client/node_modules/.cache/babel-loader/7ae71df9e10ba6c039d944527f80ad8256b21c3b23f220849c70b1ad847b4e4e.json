{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport 'vue-router';\n// import axios from 'axios';\nexport default {\n  name: \"ModalApp\",\n  computed: {\n    status() {\n      return this.$store.state.statusOverly;\n    },\n    isBlack() {\n      return this.$store.state.statusBackground;\n    }\n  },\n  methods: {\n    handleChangeBackground() {\n      this.$store.state.statusBackground = !this.$store.state.statusBackground;\n      // console.log(this.$store.state.statusBackground)\n    },\n\n    handleClickAdmin() {\n      this.$store.state.statusOverly = false;\n      if (this.$store.state.isAdmin == true) {\n        this.$router.push({\n          name: 'postblog'\n        });\n      } else {\n        this.$router.push({\n          name: 'login'\n        });\n      }\n    },\n    handlerLogout(e) {\n      e.preventDefault();\n      // //delete Token \n      this.deleteCookie('token');\n      // //isAuthentication = false\n      this.$store.state.isAuthentication = false;\n      //isAdmin =  false\n      this.$store.state.isAdmin = false;\n      //login\n      this.$router.push({\n        name: 'login'\n      });\n    },\n    deleteCookie(name) {\n      document.cookie = name + '=;expires=' + new Date(1970, 0, 1).toUTCString() + ';path=/';\n    }\n  }\n};","map":{"version":3,"mappings":";AA0CA,OAAO,YAAW;AAClB;AACA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,QAAQ,EAAE;IACRC,MAAM,GAAE;MACN,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,YAAY;IACvC,CAAC;IACDC,OAAO,GAAE;MACT,OAAO,IAAI,CAACH,MAAM,CAACC,KAAK,CAACG,gBAAgB;IACzC;EAGF,CAAC;EACDC,OAAO,EAAE;IACPC,sBAAsB,GAAE;MACtB,IAAI,CAACN,MAAM,CAACC,KAAK,CAACG,gBAAe,GAAI,CAAC,IAAI,CAACJ,MAAM,CAACC,KAAK,CAACG,gBAAgB;MACxE;IACF,CAAC;;IACDG,gBAAgB,GAAE;MAChB,IAAI,CAACP,MAAM,CAACC,KAAK,CAACC,YAAW,GAAI,KAAK;MACtC,IAAG,IAAI,CAACF,MAAM,CAACC,KAAK,CAACO,OAAM,IAAK,IAAI,EAAC;QACnC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAAEb,IAAI,EAAE;QAAW,CAAC;MACxC,CAAC,MAAI;QACH,IAAI,CAACY,OAAO,CAACC,IAAI,CAAC;UAAEb,IAAI,EAAE;QAAQ,CAAC;MAErC;IACF,CAAC;IACDc,aAAa,CAACC,CAAC,EAAC;MACdA,CAAC,CAACC,cAAc,EAAE;MAClB;MACA,IAAI,CAACC,YAAY,CAAC,OAAO;MACzB;MACA,IAAI,CAACd,MAAM,CAACC,KAAK,CAACc,gBAAe,GAAI,KAAI;MACzC;MACA,IAAI,CAACf,MAAM,CAACC,KAAK,CAACO,OAAM,GAAK,KAAI;MACjC;MACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAACb,IAAI,EAAE;MAAO,CAAC;IACnC,CAAC;IACDiB,YAAY,CAACjB,IAAI,EAAE;MACjBmB,QAAQ,CAACC,MAAK,GAAIpB,IAAG,GAAI,YAAW,GAAI,IAAIqB,IAAI,CAAC,IAAI,EAAE,CAAC,EAAE,CAAC,CAAC,CAACC,WAAW,EAAC,GAAI,SAAQ;IACvF;EACF;AACF,CAAC","names":["name","computed","status","$store","state","statusOverly","isBlack","statusBackground","methods","handleChangeBackground","handleClickAdmin","isAdmin","$router","push","handlerLogout","e","preventDefault","deleteCookie","isAuthentication","document","cookie","Date","toUTCString"],"sourceRoot":"","sources":["D:\\trainningVuejs\\blog_app_vuejs\\client\\src\\components\\Modal.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <section class=\"overlay-menu\" :class=\"{ open: status, backgroundBlack: isBlack, backgroundWhite: !isBlack}\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"main-menu\">\r\n            <ul>\r\n              <li>\r\n                <router-link :to=\"{name: 'hone'}\">Home</router-link>\r\n              </li>\r\n              <li>\r\n                <a href=\"about.html\">About Us</a>\r\n              </li>\r\n              \r\n              <li>\r\n                <a style=\"cursor: pointer\" @click=\"handleClickAdmin()\">Admin</a>\r\n              </li>\r\n\r\n              <li v-if=\"this.$store.state.isAuthentication\" class=\"btn-login\" :class=\"{ addBtnLogin: !isBlack}\">\r\n                <a @click=\"handlerLogout($event)\">Logout</a>\r\n                \r\n              </li>\r\n              <li v-else class=\"btn-login\" :class=\"{ addBtnLogin: !isBlack}\">\r\n                <router-link :to=\"{name:'login'}\">Login</router-link>\r\n              </li>\r\n              <li >\r\n                <label class=\"switch\">\r\n                  <input type=\"checkbox\" @click=\"handleChangeBackground()\">\r\n                  <span class=\"slider round\"></span>\r\n                </label>\r\n                \r\n              </li>\r\n            </ul>\r\n            <p>We create awesome templates for you</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport 'vue-router'\r\n// import axios from 'axios';\r\nexport default {\r\n  name: \"ModalApp\",\r\n  computed: {\r\n    status(){\r\n      return this.$store.state.statusOverly;\r\n    },\r\n    isBlack(){\r\n    return this.$store.state.statusBackground;\r\n    },\r\n    \r\n    \r\n  },\r\n  methods: {\r\n    handleChangeBackground(){\r\n      this.$store.state.statusBackground = !this.$store.state.statusBackground;\r\n      // console.log(this.$store.state.statusBackground)\r\n    },\r\n    handleClickAdmin(){\r\n      this.$store.state.statusOverly = false;\r\n      if(this.$store.state.isAdmin == true){\r\n        this.$router.push({ name: 'postblog' })    \r\n      }else{\r\n        this.$router.push({ name: 'login' })    \r\n\r\n      }\r\n    },\r\n    handlerLogout(e){\r\n      e.preventDefault();\r\n      // //delete Token \r\n      this.deleteCookie('token')\r\n      // //isAuthentication = false\r\n      this.$store.state.isAuthentication = false\r\n      //isAdmin =  false\r\n      this.$store.state.isAdmin =  false\r\n      //login\r\n      this.$router.push({name: 'login'})\r\n    },\r\n    deleteCookie(name) {\r\n      document.cookie = name + '=;expires=' + new Date(1970, 0, 1).toUTCString() + ';path=/'\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.switch {\r\n  position: relative;\r\n  display: inline-block;\r\n  width: 60px;\r\n  height: 34px;\r\n}\r\n\r\n.switch input { \r\n  opacity: 0;\r\n  width: 0;\r\n  height: 0;\r\n}\r\n\r\n.slider {\r\n  position: absolute;\r\n  cursor: pointer;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: #ccc;\r\n  -webkit-transition: .4s;\r\n  transition: .4s;\r\n}\r\n\r\n.slider:before {\r\n  position: absolute;\r\n  content: \"\";\r\n  height: 26px;\r\n  width: 26px;\r\n  left: 4px;\r\n  bottom: 4px;\r\n  background-color: white;\r\n  -webkit-transition: .4s;\r\n  transition: .4s;\r\n}\r\n\r\ninput:checked + .slider {\r\n  background-color: #2196F3;\r\n}\r\n\r\ninput:focus + .slider {\r\n  box-shadow: 0 0 1px #2196F3;\r\n}\r\n\r\ninput:checked + .slider:before {\r\n  -webkit-transform: translateX(26px);\r\n  -ms-transform: translateX(26px);\r\n  transform: translateX(26px);\r\n}\r\n\r\n/* Rounded sliders */\r\n.slider.round {\r\n  border-radius: 34px;\r\n}\r\n\r\n.slider.round:before {\r\n  border-radius: 50%;\r\n}\r\n\r\n.backgroundBlack{\r\n  background-color: #232323;\r\n  color: #fff;\r\n}\r\n\r\n.backgroundWhite{\r\n  background-color: #fff;\r\n  color: #000;\r\n} \r\n\r\n.addBtnLogin{\r\n  background-color: rgba(0,0,0,0.5);\r\n  color: #fff;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}