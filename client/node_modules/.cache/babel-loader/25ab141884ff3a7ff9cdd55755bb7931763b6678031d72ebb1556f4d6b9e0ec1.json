{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"PostTags\",\n  computed: {\n    isBlack() {\n      return this.$store.state.statusBackground;\n    }\n  },\n  data() {\n    return {\n      tags: \"\",\n      isError: false,\n      list_tags: [],\n      status_update: false,\n      tags_update: {\n        id: \"\",\n        name: \"\",\n        index: null\n      }\n    };\n  },\n  methods: {\n    handleSubmitTags() {\n      if (!this.tags) {\n        this.isError = true;\n      }\n      if (!this.isError) {\n        axios.post(\"http://localhost:8080/api/tags\", {\n          name: this.tags\n        }).then(response => {\n          this.list_tags.push(response.data);\n        }).catch(error => {\n          console.log(error);\n        });\n        this.status_update = false;\n        this.tags = \"\";\n      }\n    },\n    handleDeleteTags(id, index) {\n      axios.delete(\"http://localhost:8080/api/tags/\" + id).then(response => {\n        response.status == 200 && this.list_tags.splice(index, 1);\n      }).catch(error => {\n        console.log(error);\n      });\n      this.status_update = false;\n    },\n    handleGetDetail(id, index) {\n      axios.get(\"http://localhost:8080/api/tags/\" + id).then(response => {\n        if (response.status == 200) {\n          this.status_update = true;\n          this.tags_update.name = response.data.name;\n          this.tags_update.id = id;\n          this.tags_update.index = index;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    validateUpdate() {\n      if (this.tags_update.name.length == 0) {\n        this.isError = true;\n      }\n    },\n    handleUpdateTags() {\n      this.validateUpdate();\n      if (!this.isError) {\n        axios.patch(\"http://localhost:8080/api/tags/\" + this.tags_update.id, {\n          name: this.tags_update.name\n        }).then(response => {\n          if (response.status == 200) {\n            this.list_tags[this.tags_update.index].name = this.tags_update.name;\n          }\n        }).catch(error => {\n          console.log(error);\n        });\n        this.status_update = false;\n      }\n    },\n    handleUpdateStatus(id, index, status) {\n      axios.patch(\"http://localhost:8080/api/tags/\" + id, {\n        status: !status\n      }).then(response => {\n        if (response.status == 200) {\n          this.list_tags[index].status = !status;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      this.status_update = false;\n    }\n  },\n  created() {\n    axios.get(\"http://localhost:8080/api/tags\").then(response => {\n      if (response.status == 200) {\n        this.list_tags = response.data;\n      }\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":";AAiMA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,QAAQ,EAAE;IACRC,OAAO,GAAG;MACR,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,gBAAgB;IAC3C;EACF,CAAC;EACDC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE,EAAE;MACbC,aAAa,EAAE,KAAK;MACpBC,WAAW,EAAE;QACXC,EAAE,EAAE,EAAE;QACNZ,IAAI,EAAE,EAAE;QACRa,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgB,GAAG;MACjB,IAAI,CAAC,IAAI,CAACR,IAAI,EAAE;QACd,IAAI,CAACC,OAAM,GAAI,IAAI;MACrB;MAEA,IAAI,CAAC,IAAI,CAACA,OAAO,EAAE;QACjBT,KAAI,CACDiB,IAAI,CAAC,gCAAgC,EAAE;UAAEhB,IAAI,EAAE,IAAI,CAACO;QAAK,CAAC,EAC1DU,IAAI,CAAEC,QAAQ,IAAK;UAClB,IAAI,CAACT,SAAS,CAACU,IAAI,CAACD,QAAQ,CAACZ,IAAI,CAAC;QACpC,CAAC,EACAc,KAAK,CAAEC,KAAK,IAAK;UAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;QACpB,CAAC,CAAC;QACJ,IAAI,CAACX,aAAY,GAAI,KAAK;QAC1B,IAAI,CAACH,IAAG,GAAI,EAAE;MAChB;IACF,CAAC;IACDiB,gBAAgB,CAACZ,EAAE,EAAEC,KAAK,EAAE;MAC1Bd,KAAI,CACD0B,MAAM,CAAC,iCAAgC,GAAIb,EAAE,EAC7CK,IAAI,CAAEC,QAAQ,IAAK;QAClBA,QAAQ,CAACQ,MAAK,IAAK,GAAE,IAAK,IAAI,CAACjB,SAAS,CAACkB,MAAM,CAACd,KAAK,EAAE,CAAC,CAAC;MAC3D,CAAC,EACAO,KAAK,CAAEC,KAAK,IAAK;QAChB