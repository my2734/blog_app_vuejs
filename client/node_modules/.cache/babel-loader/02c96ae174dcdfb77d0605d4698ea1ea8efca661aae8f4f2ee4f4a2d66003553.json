n      margin-left: 0;\r\n      order: -1;\r\n    }\r\n    \r\n    .end,\r\n    .copyright {\r\n      text-align: start;\r\n    }\r\n  }\r\n\r\n@media(min-width: 700px) {\r\n  .page-title::after {\r\n    content: '';\r\n    display: block;\r\n    width: 160px;\r\n    height: 5px;\r\n    background: #fda039;\r\n    margin-right: auto;\r\n    margin-left: auto;\r\n    margin-top: 10px;\r\n}\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}