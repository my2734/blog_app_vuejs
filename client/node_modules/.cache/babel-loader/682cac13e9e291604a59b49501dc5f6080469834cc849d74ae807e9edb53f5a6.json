ponse = await axios.get(\"http://localhost:8080/api/post/limit/8\");\n        this.list_limit_blog8 = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    getListBlogLimit8();\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":";AAqEA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AACjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,aAAa,EAAE,EAAE;MACjBC,gBAAgB,EAAE,EAAE;MACpBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACNC,oBAAoB,CAACP,IAAI,EAACQ,CAAC,EAAC;MAC1BA,CAAC,CAACC,cAAc,EAAC;MACjB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAACX,IAAI,EAAE;MAAM,CAAC;MAChC,IAAI,CAACY,mBAAmB,CAACZ,IAAI;IAC/B,CAAC;IACDa,sBAAsB,CAACC,EAAE,EAAC;MACxB,IAAI,CAACJ,OAAO,CAACC,IAAI,CAAC;QAAEX,IAAI,EAAE,YAAY;QAAEe,MAAM,EAAE;UAAED,EAAE,EAAEA;QAAG;MAAE,CAAC;IAC9D,CAAC;IACD,GAAGf,UAAU,CAAC,CAAC,aAAa,EAAC,qBAAqB,CAAC;EACrD,CAAC;EACDiB,OAAO,GAAE;IACP,MAAMC,WAAU,GAAI,YAAY;MAC9B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAAC,gCAAgC,CAAC;QAClE,IAAI,CAACjB,SAAQ,GAAI,MAAMgB,QAAQ,CAACjB,IAAI;MACtC,EAAE,OAAOmB,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF,CAAC;IACDH,WAAW,EAAE;IACb,MAAMM,WAAU,GAAI,YAAY;MAC9B,IAAI;QACF,MAAML,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAAC,oCAAoC,CAAC;QACtE,IAAI,CAAChB,aAAY,GAAI,MAAMe,QAAQ,CAACjB,IAAI;QACxC,KAAK,IAAIuB,KAAI,IAAK,IAAI,CAACrB,aAAa,EAAE;UACpC,IAAI,CAACA,aAAa,CAACqB,KAAK,CAAC,CAACC,QAAO,GAAI,CAAC;UACtC,KAAK,IAAIC,UAAS,IAAK,IAAI,CAACxB,SAAS,EAAE;YACrC,IACE,IAAI,CAACA,SAAS,CAACwB,UAAU,CAAC,CAACC,UAAU,CAACC,QAAQ,CAC5C,IAAI,CAACzB,aAAa,CAACqB,KAAK,CAAC,CAACxB,IAAG,CAC/B,EACA;cACA,IAAI,CAACG,aAAa,CAACqB,KAAK,CAAC,CAACC,QAAQ,EAAE;YACtC;UACF;QACF;MACF,EAAE,OAAOL,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF,CAAC;IACDG,WAAW,EAAE;IAEb,MAAMM,iBAAgB,GAAI,YAAY;MACpC,IAAI;QACF,MAAMX,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAG,CAC9B,wCAAuC,CACxC;QACD,IAAI,CAACf,gBAAe,GAAIc,QAAQ,CAACjB,IAAI;MACvC,EAAE,OAAOmB,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF,CAAC;IACDS,iB