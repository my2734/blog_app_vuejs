{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"ListPost\",\n  data() {\n    return {\n      list_post: []\n    };\n  },\n  computed: {\n    isBlack() {\n      return this.$store.state.statusBackground;\n    }\n  },\n  methods: {\n    handlerDeletePost(index, id) {\n      // console.log(index,id)\n      axios.delete('http://localhost:8080/api/post/' + id).then(response => {\n        console.log(response);\n        if (response.status == 200) {\n          this.list_post.splice(index, 1);\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    handlerUpdatePost(id) {\n      this.$router.push({\n        name: 'updatepost',\n        params: {\n          id: id\n        }\n      });\n    }\n  },\n  created() {\n    axios.get('http://localhost:8080/api/post').then(response => {\n      this.list_post = response.data;\n      console.log(this.list_post);\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":";AAkJA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAE;IACJ,OAAM;MACJC,SAAQ,EAAI;IACd;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,OAAO,GAAG;MACR,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,gBAAgB;IAC3C;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,iBAAiB,CAACC,KAAK,EAACC,EAAE,EAAC;MACzB;MACAZ,KAAK,CAACa,MAAM,CAAC,iCAAiC,GAACD,EAAE,EAChDE,IAAI,CAACC,QAAQ,IAAE;QACdC,OAAO,CAACC,GAAG,CAACF,QAAQ;QACpB,IAAGA,QAAQ,CAACG,MAAK,IAAK,GAAG,EAAC;UACxB,IAAI,CAACf,SAAS,CAACgB,MAAM,CAACR,KAAK,EAAC,CAAC;QAC/B;MACF,CAAC,CAAC,CAACS,KAAK,CAACC,KAAK,IAAE;QACdL,OAAO,CAACC,GAAG,CAACI,KAAK;MACnB,CAAC;IACH,CAAC;IACDC,iBAAiB,CAACV,EAAE,EAAC;MACnB,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC;QAACvB,IAAI,EAAE,YAAY;QACvCwB,MAAM,EAAE;UAAEb,EAAE,EAAEA;QAAG;MAAC,CAAC;IACnB;EACF,CAAC;EACDc,OAAO,GAAE;IACP1B,KAAK,CAAC2B,GAAG,CAAC,gCAAgC,EACzCb,IAAI,CAACC,QAAQ,IAAE;MACd,IAAI,CAACZ,SAAQ,GAAIY,QAAQ,CAACb,IAAI;MAC9Bc,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,SAAS,CAAC;IAC